<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/background_degradado_principal"
    >

    <TextView
        android:id="@+id/textViewBaile"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/btnBaile"
        android:visibility="invisible"
        />

    <TextView
        android:id="@+id/textViewParaBaila"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/btnParaBaile"
        android:visibility="invisible"
        />

    <TextView
        android:id="@+id/textViewTemperatura"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/temperatura"
        android:visibility="invisible"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
